# Discord Bot Token (dari https://discord.com/developers/applications)
DISCORD_TOKEN=your_bot_token_here

# Client ID bot kamu
CLIENT_ID=your_client_id_here

# Guild ID server kamu (untuk deploy slash commands lebih cepat)
GUILD_ID=your_guild_id_here

# Prefix untuk command (default: !)
PREFIX=!

# Spotify credentials (opsional, untuk support Spotify)
# Daftar di https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
